{
  "name": "rhp",
  "version": "1.0.0",
  "description": "Azure Functions sample for the Serverless framework",
  "keywords": [
    "azure",
    "serverless"
  ],
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "ava": "^0.25.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "concurrently": "^4.0.1",
    "nock": "^10.0.1",
    "nyc": "^13.0.1",
    "onchange": "^5.0.2",
    "proxyquire": "^2.1.0",
    "serverless": "^1.32.0",
    "serverless-azure-functions": "^0.7.0",
    "sinon": "^6.3.5",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1"
  },
  "dependencies": {
    "async": "^2.6.1",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "documentdb": "^1.14.5",
    "documentdb-session": "^1.0.1",
    "dotenv": "^6.1.0",
    "enigma": "^1.1.2",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "helmet": "^3.15.0",
    "lodash": "^4.17.11",
    "mongoose": "^5.3.5",
    "passport": "^0.4.0",
    "passport-azure-ad": "^4.0.0",
    "pg": "^7.5.0",
    "pg-connection-string": "^2.0.0",
    "request": "^2.88.0"
  },
  "scripts": {
    "start": "node src/server/",
    "test": "concurrently 'standard --fix --verbose | snazzy' 'nyc ava'",
    "watch": "onchange '**/*.js' -- npm run test"
  },
  "babel": {
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./"
          ],
          "alias": {
            "connect": "./src/server/connect",
            "constant": "./test/constant",
            "call_minut_api": "./src/server/call_minut_api",
            "insert_into_cosmos": "./src/server/insert_into_cosmos",
            "insert_into_postgres": "./src/server/insert_into_postgres",
            "get_data_cosmos": "./src/server/get_data_cosmos",
            "db": "./src/server/db"
          },
          "transformFunctions": [
            "proxyquire",
            "resolvePath"
          ]
        }
      ]
    ]
  },
  "ava": {
    "files": [
      "test/unit/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": true,
    "reporter": [
      "html",
      "text-summary"
    ]
  }
}
