  "scripts": {
    "build": "ng build",
    "start": "node src/server/",
    "test": "standard --fix --verbose | snazzy",
    "watch": "onchange '**/*.js' -- npm run test"
  },
  "babel": {
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./"
          ],
          "alias": {
            "connect": "./src/server/connect",
            "constant": "./test/constant",
            "call_minut_api": "./src/server/call_minut_api",
            "insert_into_cosmos": "./src/server/insert_into_cosmos",
            "insert_into_postgres": "./src/server/insert_into_postgres",
            "get_data_cosmos": "./src/server/get_data_cosmos",
            "db": "./src/server/db"
          },
          "transformFunctions": [
            "proxyquire",
            "resolvePath"
          ]
        }
      ]
    ]
  },
  "ava": {
    "files": [
      "test/unit/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": true,
    "reporter": [
      "html",
      "text-summary"
    ]
  }
